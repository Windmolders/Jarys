<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="clastic_backoffice_contact_list" path="/contact/list">
        <default key="_controller">JwiContactBundle:ContactData:list</default>
    </route>

    <route id="clastic_backoffice_contact_form" path="/contact/edit/{id}">
        <default key="_controller">JwiContactBundle:ContactData:form</default>
        <default key="id" xsi:nil="true" />
    </route>

    <route id="clastic_backoffice_contact_delete" path="/contact/delete/{id}">
        <default key="_controller">JwiContactBundle:ContactData:delete</default>
    </route>

</routes>
