<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="contact-form-data" path="/contact-form-data/list">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:index</default>
        <default key="default">true</default>
        <default key="name">true</default>
    </route>

    <route id="contact-form-data_show" path="/contact-form-data/{id}/show">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:show</default>
    </route>

    <route id="contact-form-data_new" path="/contact-form-data/new">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:new</default>
    </route>

    <route id="contact-form-data_create" path="/contact-form-data/create" methods="POST">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:create</default>
    </route>

    <route id="contact-form-data_edit" path="/contact-form-data/{id}/edit-record">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:edit</default>
    </route>

    <route id="contact-form-data_update" path="/contact-form-data/{id}/update" methods="POST|PUT">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:update</default>
    </route>

    <route id="contact-form-data_delete" path="/contact-form-data/{id}/delete" methods="POST|DELETE">
        <default key="_controller">ClasticContactFormBundle:ContactFormData:delete</default>
    </route>

    <route id="contact-form-type" path="/contact-form-type/">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:index</default>
    </route>

    <route id="contact-form-type_show" path="/contact-form-type/{id}/show">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:show</default>
    </route>

    <route id="contact-form-type_new" path="/contact-form-type/new">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:new</default>
    </route>

    <route id="contact-form-type_create" path="/contact-form-type/create" methods="POST">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:create</default>
    </route>

    <route id="contact-form-type_edit" path="/contact-form-type/{id}/edit-record">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:edit</default>
    </route>

    <route id="contact-form-type_update" path="/contact-form-type/{id}/update" methods="POST|PUT">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:update</default>
    </route>

    <route id="contact-form-type_delete" path="/contact-form-type/{id}/delete" methods="POST|DELETE">
        <default key="_controller">ClasticContactFormBundle:ContactFormType:delete</default>
    </route>

</routes>
